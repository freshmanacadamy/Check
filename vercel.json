{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/bot.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/bot.js"
    }
  ],
  "functions": {
    "api/bot.js": {
      "maxDuration": 30
    }
  }
}
